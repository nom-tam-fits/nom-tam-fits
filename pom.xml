          <sourceFileExcludes>
            <exclude>**/CompressionLibLoaderProtection.java</exclude>
            <exclude>**/CloseIS.java</exclude>
            <exclude>**/ICompressor*.java</exclude>
            <exclude>**/HCompress.java</exclude>
            <exclude>**/HDecompress.java</exclude>
