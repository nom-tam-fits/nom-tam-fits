      <distributionManagement>
        <repository>
          <id>nexus</id>
          <name>i4j sonatype Maven 2 repository</name>
          <url>https://oss.sonatype.org/service/local/staging/deploy/maven2</url>
        </repository>
      </distributionManagement>
    </profile>
    
    <profile>
      <id>github-repo</id>
      <distributionManagement>
        <repository>
          <id>github</id>
          <name>GitHub nom-tam-fits Apache Maven Packages</name>
          <url>https://maven.pkg.github.com/nom-tam-fits/nom-tam-fits</url>
        </repository>
      </distributionManagement>
    </profile>
    
    <profile>
      <id>skipBlackBoxImages</id>
      <properties>
        <skip.backbox.images>true</skip.backbox.images>
      </properties>
    </profile>
    
    <profile>
      <id>public-api</id>
      <build>
        <plugins>
          <plugin>
            <groupId>org.apache.maven.plugins</groupId>
            <artifactId>maven-javadoc-plugin</artifactId>
            <configuration>
              <source>8</source>
              <failOnError>false</failOnError>
              <aggregate>false</aggregate>
              <doctitle>
                ${project.name} ${project.version} Public User API
              </doctitle>
              <windowtitle>
                ${project.name} ${project.version} Public User API
              </windowtitle>
              <show>public</show>
            </configuration>
          </plugin>
        </plugins>
      </build>
    </profile>
    
  </profiles>
  
</project>
